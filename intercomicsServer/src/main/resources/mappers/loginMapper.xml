<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.intercomics.mapper.LoginMapper">

	<insert id="create">
		insert into member 
		values (#{userId}, #{userToken}, #{mail}, #{nickname}, #{profileImage}, #{gender}, #{id}, #{name}, #{birthday}, #{age})
	</insert>
</mapper>